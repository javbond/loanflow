# LoanFlow Docker Environment Configuration
# Copy to .env and customize

# PostgreSQL
POSTGRES_USER=loanflow
POSTGRES_PASSWORD=loanflow_secret
POSTGRES_PORT=5432

# MongoDB
MONGO_USER=loanflow
MONGO_PASSWORD=loanflow_secret
MONGO_PORT=27017

# Redis
REDIS_PASSWORD=loanflow_secret
REDIS_PORT=6379

# Keycloak
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KEYCLOAK_PORT=8180

# Flowable
FLOWABLE_PORT=8085

# MinIO
MINIO_USER=loanflow
MINIO_PASSWORD=loanflow_secret
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001

# RabbitMQ
RABBITMQ_USER=loanflow
RABBITMQ_PASSWORD=loanflow_secret
RABBITMQ_PORT=5672
RABBITMQ_MGMT_PORT=15672
